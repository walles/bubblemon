msgid "$HOME environment variable not set\n"
msgstr "Umhverfisbreytan $HOME finnst ekki\n"

#: src/bubblemon.c:663
#, c-format
msgid ""
"Bubblemon exiting.  Profiling data should be in ~/gmon.out.\n"
"For a good time, run 'gprof -l -p %s ~/gmon.out'.\n"
msgstr ""
"Bubblemon hættir. Prófílgögn liggja í ~/gmon.out\n"
"Keyrið 'gprof -l -p %s ~/gmon.out' til að sjá tölulegar upplýsingar.\n"

